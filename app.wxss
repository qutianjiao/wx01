@import "./styles/iconfont.wxss";

page,view,text,swiper,swiper-item,image,navigator{
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}

/* 
主题颜色 通过变量来实现
1 less中 存在 变量这个知识
2 原生的css和wxss也支持 变量
*/
page{
    /* 定义主题颜色 */
    --themeColor:#0e4b9d;
    /* 定义统一字体大小  */
    font-size: 30rpx;
}
